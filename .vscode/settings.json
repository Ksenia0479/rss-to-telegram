  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.alwaysShowStatus": true,
  "markdownlint.config": {
    "MD041": false
  },
  "cSpell.words": [
    "bullmq",
    "datasource",
    "dbgenerated",
    "dbsync",
    "deunionize",
    "einaregilsson",
    "Openai",
    "pullquote",
    "tlang",
    "tlanguage",
    "typegram",
    "unposted"
  ]
}
